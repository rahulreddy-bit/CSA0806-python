arr = [int(x) for x in input("Enter elements separated by space: ").split()]
duplicates = []
seen = set()
for item in arr:
    if item in seen:
        if item not in duplicates:
            duplicates.append(item)
    else:
        seen.add(item)

if duplicates:
    print("Duplicate elements are:", duplicates)
else:
    print("No duplicates found.")
